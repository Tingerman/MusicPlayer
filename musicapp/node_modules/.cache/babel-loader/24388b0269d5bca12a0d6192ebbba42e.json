{"ast":null,"code":"import { onMounted } from 'vue';\nexport default {\n  props: ['playlist']\n};","map":{"version":3,"mappings":"AA+KA,SAASA,SAAT,QAA0B,KAA1B;AAEA,eAAe;EACXC,KAAK,EAAC,CAAC,UAAD;AADK,CAAf","names":["onMounted","props"],"sourceRoot":"","sources":["/Users/wangmanting/Desktop/云e办/音乐播放器vue项目/musicapp/src/components/listviewTop.vue"],"sourcesContent":["<template>\n    <div class=\"listViewTop\">\n        <img class=\"bg\" :src=\"playlist.coverImgUrl\" alt=\"\">\n        <div class=\"listViewTopNav\">\n            <div class=\"back\">\n                <svg class=\"icon\" aria-hidden=\"true\">\n                    <use xlink:href=\"#icon-guanbi\"></use>\n                </svg>\n                <div class=\"title\">\n                    歌单\n                </div>\n            </div>\n\n            <div class=\"right\">\n                <svg class=\"icon search\" aria-hidden=\"true\">\n                    <use xlink:href=\"#icon-sousuoxiao-copy\"></use>\n                </svg>\n                <svg class=\"icon more\" aria-hidden=\"true\">\n                    <use xlink:href=\"#icon-diandiandianshu\"></use>\n                </svg>\n\n            </div>\n\n        </div>\n\n        <div class=\"content\">\n            <div class=\"contentLeft\">\n                <img :src=\"playlist.coverImgUrl\" alt=\"\">\n                <div class=\"count\">\n                    <!-- <svg class=\"icon\" aria-hidden=\"true\">\n                        <use xlink:href=\"#icon-bofang2-copy-copy\"></use>\n                    </svg> -->\n                    <span class=\"symbol-play\">▷</span>\n                    <span>{{parseInt(playlist.playCount/10000)}}万</span>\n                </div>\n            </div>\n            <div class=\"contentRight\">\n                <h3>{{playlist.name}}</h3>\n                <div class=\"author\">\n                    <img :src=\"playlist.creator.avatarUrl\" class=\"avatar\">\n                    <span>{{playlist.creator.nickname}}</span>\n                </div>\n                <div class=\"description\">\n                    {{playlist.description}}\n                </div>\n\n            </div>\n        </div>\n\n        <div class=\"iconList\">\n            <div class=\"iconItem\">\n                <svg class=\"icon\" aria-hidden=\"true\">\n                    <use xlink:href=\"#icon-bofang2-copy-copy\"></use>\n                </svg>\n                <span>▷</span>\n            </div>\n            <div class=\"iconItem\"></div>\n            <div class=\"iconItem\"></div>\n            <div class=\"iconItem\"></div>\n        </div>\n    </div>\n</template>\n\n<style lang=\"less\">\n.listViewTop{\n    width:7.5rem;\n    padding:0 0.4rem;\n    height:6rem;\n    .bg{\n        position:fixed;\n        left: 0;\n        top:0;\n        width:7.5rem;\n        height: auto;\n        z-index:-1;\n        filter:blur(40px);\n    }\n\n    .listViewTopNav{\n        display:flex;\n        justify-content: space-between;\n        height: 1.2rem;\n        align-items: center;\n        font-size: 0.35rem;\n        .icon{\n            width:0.4rem;\n            height: 0.4rem;\n            fill:#fff;\n        }\n        .title{\n            margin-left:0.4rem;\n\n        }\n        .search{\n            margin-right: 0.5rem;\n        }\n        .more{\n            height: 0.5rem;\n            width:0.5rem;\n            margin-top: -0.05rem;\n        }\n\n\n    }\n    .back,.right{\n        display: flex;\n        color:#fff;\n    }\n    \n    .content{\n        display: flex;\n        justify-content: space-between;\n        padding:0.6rem 0;\n        .contentLeft{\n            position:relative;\n            img{\n                width: 2.8rem;\n                height: 2.8rem;\n                border-radius: 0.1rem;\n            }\n            .count{\n                position: absolute;\n                right:0.05rem;\n                top:0.1rem;\n                font-size: 0.04rem;\n                color:#e6e6e6;\n                margin-right:0.05rem;\n                text-shadow: 0.01rem 0.01rem #333 ;\n                .icon{\n                    // fill:rgba(255, 255, 255, 0.776);\n                    fill:#e6e6e6;\n                    height:0.25rem;\n                    width:0.25rem;\n                    margin-right:0.02rem;\n                }\n                .symbol-play{\n                    margin-right:0.03rem;\n                    // font-weight: 900;\n                    font-size:0.01rem;\n                }\n            }\n        }\n        .contentRight{\n            width:3.5rem;\n            h3{\n                color:white;\n            }\n            .author{\n                display: flex;\n                align-items: center;\n                color:white;\n                margin: 0.2rem 0;\n                font-size: 0.28rem;\n                img.avatar{\n                    width:0.6rem;\n                    height: 0.6rem;\n                    border-radius: 50%;\n                    margin-right:0.16rem;\n                }\n            }\n            .description{\n                font-size:0.24rem;\n                color:white;\n                height: 0.7rem;\n                overflow: hidden;\n            }\n\n        }\n    }\n}\n\n</style>\n\n<script>\n\nimport { onMounted } from 'vue';\n\nexport default {\n    props:['playlist'],\n}\n</script>"]},"metadata":{},"sourceType":"module"}