{"ast":null,"code":"import { onUpdated, ref, watch } from 'vue';\nimport { mapState, mapMutations } from 'vuex';\nexport default {\n  computed: { ...mapState(['playlist', 'playCurrentIndex'])\n  },\n\n  mounted() {\n    console.log(this.$refs);\n  },\n\n  data() {\n    return {\n      paused: true\n    };\n  },\n\n  methods: {\n    play: function () {\n      if (this.$refs.audio.paused) {\n        this.$refs.audio.play();\n        this.paused = false;\n      } else {\n        this.$refs.audio.pause();\n        this.paused = true;\n      }\n    }\n  },\n\n  setup() {\n    let audio = ref(null);\n    let audio_reactive = reactive({\n      audio: {}\n    });\n    onMounted(async () => {\n      let result = await ref(null);\n      audio.playlist = result.data.playlist;\n      store.commit('setPlaylist', state.playlist.tracks); // console.log(\"父页面查询结果：\",result);\n\n      console.log(\"store里传值成功了吗？\", store.state.playlist); // console.log(\"父页面传值playlist：\",state.playlist);\n    });\n    return {\n      state\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAyBA,SAASA,SAAT,EAAmBC,GAAnB,EAAuBC,KAAvB,QAAoC,KAApC;AACA,SAAQC,QAAR,EAAiBC,YAAjB,QAAoC,MAApC;AACA,eAAe;EACXC,QAAQ,EAAC,EACL,GAAGF,QAAQ,CAAC,CAAC,UAAD,EAAY,kBAAZ,CAAD;EADN,CADE;;EAIXG,OAAO,GAAE;IAELC,OAAO,CAACC,GAAR,CAAY,KAAKC,KAAjB;EACH,CAPU;;EAQXC,IAAI,GAAE;IACF,OAAM;MACFC,MAAM,EAAC;IADL,CAAN;EAGH,CAZU;;EAaXC,OAAO,EAAC;IACJC,IAAI,EAAC,YAAU;MACX,IAAG,KAAKJ,KAAL,CAAWK,KAAX,CAAiBH,MAApB,EAA2B;QACvB,KAAKF,KAAL,CAAWK,KAAX,CAAiBD,IAAjB;QACA,KAAKF,MAAL,GAAY,KAAZ;MACH,CAHD,MAGK;QACD,KAAKF,KAAL,CAAWK,KAAX,CAAiBC,KAAjB;QACA,KAAKJ,MAAL,GAAY,IAAZ;MACJ;IACJ;EATI,CAbG;;EAyBXK,KAAK,GAAE;IACH,IAAIF,KAAI,GAAIb,GAAG,CAAC,IAAD,CAAf;IACA,IAAIgB,cAAc,GAACC,QAAQ,CAAC;MAACJ,KAAK,EAAC;IAAP,CAAD,CAA3B;IACAK,SAAS,CAAC,YAAS;MACf,IAAIC,MAAM,GAAC,MAAMnB,GAAG,CAAC,IAAD,CAApB;MACAa,KAAK,CAACO,QAAN,GAAeD,MAAM,CAACV,IAAP,CAAYW,QAA3B;MACAC,KAAK,CAACC,MAAN,CAAa,aAAb,EAA2BC,KAAK,CAACH,QAAN,CAAeI,MAA1C,EAHe,CAIf;;MACAlB,OAAO,CAACC,GAAR,CAAY,eAAZ,EAA4Bc,KAAK,CAACE,KAAN,CAAYH,QAAxC,EALe,CAMf;IACH,CAPQ,CAAT;IASA,OAAO;MACHG;IADG,CAAP;EAIJ;;AAzCW,CAAf","names":["onUpdated","ref","watch","mapState","mapMutations","computed","mounted","console","log","$refs","data","paused","methods","play","audio","pause","setup","audio_reactive","reactive","onMounted","result","playlist","store","commit","state","tracks"],"sourceRoot":"","sources":["/Users/wangmanting/Desktop/云e办/音乐播放器vue项目/musicapp/src/components/playController.vue"],"sourcesContent":["<template>\n    <div class=\"playController\">\n        <div class=\"left\">\n            <img :src=\"playlist[playCurrentIndex].al.picUrl\" alt=\"\">\n            <div class=\"content\">\n                <div class=\"title\">{{playlist[playCurrentIndex].name}}</div>\n                <div class=\"tips\">横滑可以切换上下首歌</div>\n            </div>\n        </div>\n        <div class=\"right\">\n            <svg class=\"icon play\" v-if=\"paused\" aria-hidden=\"true\" @click=\"play\">\n                <use xlink:href=\"#icon-24gl-playCircle\"></use>\n            </svg>\n            <svg class=\"icon pause\" v-else aria-hidden=\"true\" @click=\"play\">\n                <use xlink:href=\"#icon-zanting\"></use>\n            </svg>  \n            <svg class=\"icon more\" aria-hidden=\"true\">\n                <use xlink:href=\"#icon-liebiao\"></use>\n            </svg>\n        </div>\n        <audio ref=\"audio\" :src=\"`https://music.163.com/song/media/outer/url?id=${playlist[playCurrentIndex].id}.mp3`\"></audio>\n    </div>\n</template>\n\n<script>\nimport { onUpdated,ref,watch } from 'vue';\nimport {mapState,mapMutations} from 'vuex'\nexport default {\n    computed:{\n        ...mapState(['playlist','playCurrentIndex'])\n    },\n    mounted(){\n\n        console.log(this.$refs)\n    },\n    data(){\n        return{\n            paused:true\n        }\n    },\n    methods:{\n        play:function(){\n            if(this.$refs.audio.paused){\n                this.$refs.audio.play();\n                this.paused=false;\n            }else{\n                this.$refs.audio.pause();\n                this.paused=true;\n            }\n        }\n    },\n\n    setup(){\n        let audio = ref(null);\n        let audio_reactive=reactive({audio:{}});\n        onMounted(async()=>{\n            let result=await ref(null);\n            audio.playlist=result.data.playlist;\n            store.commit('setPlaylist',state.playlist.tracks);\n            // console.log(\"父页面查询结果：\",result);\n            console.log(\"store里传值成功了吗？\",store.state.playlist)\n            // console.log(\"父页面传值playlist：\",state.playlist);\n        })\n\n        return {\n            state\n        }\n\n    }\n    \n\n\n}\n</script>\n\n<style scoped lang=\"less\">\n.playController{\n    background-color: white;\n    border-top:0.4px solid #ccc;\n    width: 7.5rem;\n    height: 1.2rem;\n    position: fixed;\n    left:0;\n    bottom: 0;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    .left{\n        display: flex;\n        flex-direction: row;\n        .content{\n            display: flex;\n            flex-direction: column;\n            width:4rem;\n            .title{\n                height: 0.4rem;\n                overflow: hidden;\n            }\n\n            .tips{\n                color:rgb(120, 120, 120);\n                font-size: 0.24rem;\n                margin-top:0.04rem;\n            }\n        }\n        img{\n            width:0.8rem;\n            height: 0.8rem;\n            border-radius: 50%;\n            margin-left: 0.4rem;\n            margin-right:0.3rem;\n        }\n\n    }\n    .right{\n        display: flex;\n        flex-direction: row;\n        margin-right: 0.6rem;\n        .icon{\n            width:0.5rem;\n            height: 0.5rem;\n            fill:#333;\n            // margin-left:0.4rem;\n        }\n        .more{\n            margin-left:0.4rem;\n        }\n        .pause{\n            width: 0.45rem;\n            height: 0.45rem;\n            position: relative;\n            right:0.02rem;\n            bottom:-0.02rem;\n        }\n    }\n\n}\n\n</style>"]},"metadata":{},"sourceType":"module"}