{"ast":null,"code":"import axios from 'axios';\nimport { getMusicList } from '@/api/index';\nexport default {\n  data() {\n    return {\n      //定义跑马灯的图片路径\n      imgs: [{\n        coverImgUrl: require(\"../assets/joshua.png\")\n      }, {\n        coverImgUrl: require(\"../assets/daniela.webp\")\n      }, {\n        coverImgUrl: require(\"../assets/roysterlee2.webp\")\n      }]\n    };\n  },\n\n  async mounted() {\n    let res = await getMusicList(); //默认是get方法\n\n    console.log(res);\n    this.imgs = res.data.playlists;\n  }\n\n};","map":{"version":3,"mappings":"AAiGA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,YAAR,QAA2B,aAA3B;AACA,eAAe;EACXC,IAAI,GAAG;IACP,OAAO;MACL;MACAC,IAAI,EAAC,CACH;QAAEC,WAAW,EAAEC,OAAO,CAAC,sBAAD;MAAtB,CADG,EAEH;QAAED,WAAW,EAAEC,OAAO,CAAC,wBAAD;MAAtB,CAFG,EAGH;QAAED,WAAW,EAAEC,OAAO,CAAC,4BAAD;MAAtB,CAHG;IAFA,CAAP;EAQC,CAVU;;EAYX,MAAMC,OAAN,GAAe;IACX,IAAIC,GAAG,GAAC,MAAMN,YAAY,EAA1B,CADW,CACkB;;IAC7BO,OAAO,CAACC,GAAR,CAAYF,GAAZ;IACA,KAAKJ,IAAL,GAAUI,GAAG,CAACL,IAAJ,CAASQ,SAAnB;EACJ;;AAhBW,CAAf","names":["axios","getMusicList","data","imgs","coverImgUrl","require","mounted","res","console","log","playlists"],"sourceRoot":"","sources":["/Users/wangmanting/Desktop/云e办/音乐播放器vue项目/musicapp/src/components/musicList.vue"],"sourcesContent":["<template>\n    <div class=\"musicList\">\n        <div class=\"musicList-top\">\n            <div class=\"title\">发现好歌单</div>\n            <div class=\"more\">查看更多</div>\n        </div>\n\n        <div class=\"mlist\">\n            <!-- 可以轮播 -->\n              <el-carousel :interval=\"4000\" type=\"card\" height=\"200px\" indicator-position=\"none\">\n                <el-carousel-item v-for=\"item in imgs\" :key=\"item\" label=\"{{item.name}}\">\n                <!-- <h3 text=\"2xl\" justify=\"center\">{{ item }}</h3> -->\n                <div class=\"mlitem\">\n                    <div class=\"count\">{{item.playCount/10000}}wan</div>\n                    <img class=\"mlimg\" :src=\"item.coverImgUrl\" alt=\"\"/>\n                    <div class=\"covername\">{{item.name}}</div>\n                </div>\n                </el-carousel-item>\n            </el-carousel>\n\n        </div>\n    </div>\n</template>\n\n<style lang=\"less\" scoped>\n.musicList{\n    width:7.5rem;\n    /* margin:0.2rem; */\n    padding:0 0.3rem;\n    .musicList-top{\n        display: flex;\n        justify-content: space-between;\n        height:0.6rem;\n        .title{\n            font-size:0.35rem;\n            font-weight: 900;\n            padding-left: 0.1rem;\n        }\n        .more{\n            font-size:0.26rem;\n            padding-top: 0.09rem;\n            padding-right:0.1rem;\n        }\n    }\n\n    .mlist{\n        width:8rem;\n\n        .mlitem{\n            \n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            justify-content: center;\n            text-align: start;\n\n            .mlimg{\n                height: 80%;\n                width:80%;\n                margin:0.1rem;\n                border-radius: 0.1rem;\n            }\n            .covername{\n                margin:0.02rem 0.04rem;\n                font-size: 0.04rem;\n                height: 30%;\n                width:80%;\n                text-align: center;\n                \n            }\n            .count{\n                position:absolute;\n                right:0.3rem;\n                top:0.3rem;\n                font-size: 0.04rem;\n            }\n        }\n    }\n}\n\n.el-carousel__item{\n    height: 2.8rem;\n    width: 2.8rem;\n    border-radius: 0.1rem;\n}\n\n.el-carousel__item:nth-child(2n) {\n  background-color: white;\n//   background-color: skyblue;\n}\n\n.el-carousel__item:nth-child(2n + 1) {\n  background-color: white;\n}\n</style>\n\n<script>\nimport axios from 'axios';\nimport {getMusicList} from '@/api/index'\nexport default {\n    data() {\n    return {\n      //定义跑马灯的图片路径\n      imgs:[\n        { coverImgUrl: require(\"../assets/joshua.png\") },\n        { coverImgUrl: require(\"../assets/daniela.webp\") },\n        { coverImgUrl: require(\"../assets/roysterlee2.webp\") }, \n      ]\n    };\n    },\n\n    async mounted(){\n        let res=await getMusicList();//默认是get方法\n        console.log(res)\n        this.imgs=res.data.playlists;\n    }\n\n}\n</script>"]},"metadata":{},"sourceType":"module"}