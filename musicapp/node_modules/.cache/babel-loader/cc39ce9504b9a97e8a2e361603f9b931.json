{"ast":null,"code":"import listviewTop from '@/components/listviewTop.vue';\nimport playList from '@/components/playList.vue';\nimport { getPlayListDetail } from '@/api/index';\nimport { onMounted, reactive } from 'vue';\nimport { useRoute } from 'vue-router';\nimport store from '@';\nexport default {\n  setup() {\n    const route = useRoute();\n    let state = reactive({\n      list: [],\n      playlist: {\n        creator: {// avatarUrl: \"http://p1.music.126.net/qRFbvij0Bc6_b2fqzLAP3w==/109951167601809187.jpg\",\n          // backgroundImgId: 109951166640164350,\n          // backgroundImgIdStr: \"109951166640164345\",\n          // backgroundUrl: \"http://p1.music.126.net/Tb8Ktmkz3TOqnupwnxaxLQ==/109951166640164345.jpg\",\n          // birthday: 0,\n          // nickname: \"0000FF少抽烟\"\n        },\n        trackCount: -1,\n        tracks: []\n      }\n    });\n    onMounted(async () => {\n      let id = route.query.id;\n      let result = await getPlayListDetail(id);\n      state.playlist = result.data.playlist;\n      store.commit('setPlaylist', state.playlist.tracks);\n      console.log(\"父页面查询结果：\", result); // console.log(\"父页面传值playlist：\",state.playlist);\n    });\n    return {\n      state\n    };\n  },\n\n  components: {\n    listviewTop,\n    playList\n  }\n};","map":{"version":3,"mappings":"AAQA,OAAOA,WAAP,MAAwB,8BAAxB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AAEA,SAASC,iBAAT,QAAkC,aAAlC;AACA,SAAQC,SAAR,EAAkBC,QAAlB,QAAiC,KAAjC;AACA,SAAQC,QAAR,QAAuB,YAAvB;AAEA,OAAOC,KAAP,MAAkB,GAAlB;AAEA,eAAe;EACXC,KAAK,GAAE;IACH,MAAMC,KAAK,GAACH,QAAQ,EAApB;IACA,IAAII,KAAK,GAACL,QAAQ,CAAC;MACfM,IAAI,EAAC,EADU;MAEfC,QAAQ,EAAC;QACLC,OAAO,EAAC,CACJ;UACA;UACA;UACA;UACA;UACA;QANI,CADH;QASLC,UAAU,EAAC,CAAC,CATP;QAULC,MAAM,EAAC;MAVF;IAFM,CAAD,CAAlB;IAgBAX,SAAS,CAAC,YAAS;MACf,IAAIY,EAAE,GAACP,KAAK,CAACQ,KAAN,CAAYD,EAAnB;MACA,IAAIE,MAAM,GAAC,MAAMf,iBAAiB,CAACa,EAAD,CAAlC;MACAN,KAAK,CAACE,QAAN,GAAeM,MAAM,CAACC,IAAP,CAAYP,QAA3B;MACAL,KAAK,CAACa,MAAN,CAAa,aAAb,EAA2BV,KAAK,CAACE,QAAN,CAAeG,MAA