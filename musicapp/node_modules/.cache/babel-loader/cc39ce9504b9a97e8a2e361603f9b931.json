            // console.log(\"父页面传值playlist：\",state.playlist);\n        })\n\n        return {\n            state\n        }\n    },\n    components:{\n        listviewTop,\n        playList\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}