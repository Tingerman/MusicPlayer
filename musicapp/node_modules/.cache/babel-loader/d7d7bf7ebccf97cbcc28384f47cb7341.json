  <div class=\"back\" @click=\"$router.go(-1)\">\n                <svg class=\"icon\" aria-hidden=\"true\">\n                    <use xlink:href=\"#icon-guanbi\"></use>\n                </svg>\n            </div>\n            <div class=\"inputbox\">\n                <input type=\"text\" v-model=\"searchKeyword\" :placeholder=\"placeholder\" @keydown.enter=\"saveKeyword\">\n            </div>\n\n            <!-- <div class=\"right\">\n                <svg class=\"icon search\" aria-hidden=\"true\">\n                    <use xlink:href=\"#icon-sousuoxiao-copy\"></use>\n                </svg>\n            </div> -->\n\n        </div>\n\n        <div class=\"history\">\n            <div class=\"historyLeft\">历史</div>\n            <div class=\"historyRight\">\n                <div class=\"keywordItem\" v-for=\"(item,i) in keywordList\" :key=\"i\">{{item}}</div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<style lang=\"less\">\n.listViewTop{\n    width:7.5rem;\n    padding:0 0.4rem;\n    // height:6rem;\n    .bg{\n        position:fixed;\n        left: 0;\n        top:0;\n        width:7.5rem;\n        height: auto;\n        z-index:-1;\n        filter:blur(60px);\n    }\n\n    .listViewTopNav{\n        display:flex;\n        justify-content: space-between;\n        height: 1.2rem;\n        align-items: center;\n        font-size: 0.35rem;\n        .icon{\n            width:0.4rem;\n            height: 0.4rem;\n            fill:#fff;\n        }\n        .title{\n            margin-left:0.4rem;\n\n        }\n        .search{\n            margin-right: 0.5rem;\n        }\n        .more{\n            height: 0.5rem;\n            width:0.5rem;\n            margin-top: -0.05rem;\n        }\n\n\n    }\n    .back,.right{\n        display: flex;\n        color:#fff;\n    }\n    \n    .content{\n        display: flex;\n        justify-content: space-between;\n        padding:0.6rem 0;\n        .contentLeft{\n            position:relative;\n            img{\n                width: 2.8rem;\n                height: 2.8rem;\n                border-radius: 0.1rem;\n            }\n            .count{\n                position: absolute;\n                right:0.05rem;\n                top:0.1rem;\n                font-size: 0.04rem;\n                color:#e6e6e6;\n                margin-right:0.05rem;\n                text-shadow: 0.01rem 0.01rem #333 ;\n                .icon{\n                    // fill:rgba(255, 255, 255, 0.776);\n                    fill:#e6e6e6;\n                    height:0.25rem;\n                    width:0.25rem;\n                    margin-right:0.02rem;\n                }\n                .symbol-play{\n                    margin-right:0.03rem;\n                    // font-weight: 900;\n                    font-size:0.01rem;\n                }\n            }\n        }\n        .contentRight{\n            width:3.5rem;\n            h3{\n                color:white;\n            }\n            .author{\n                display: flex;\n                align-items: center;\n                color:white;\n                margin: 0.2rem 0;\n                font-size: 0.28rem;\n                img.avatar{\n                    width:0.6rem;\n                    height: 0.6rem;\n                    border-radius: 50%;\n                    margin-right:0.16rem;\n                }\n            }\n            .description{\n                font-size:0.24rem;\n                color:white;\n                height: 0.7rem;\n                overflow: hidden;\n            }\n\n        }\n    }\n\n    .iconList{\n        padding:0 0.5rem;\n        .iconItem{\n            color:white;\n            \n            .icon{\n                fill:white;\n                width: 0.5rem;\n            }\n            span{\n                font-size:0.24rem;\n                margin-top:0.1rem;\n            }\n        }\n    }\n}\n\n</style>\n\n<script>\n\nimport { beforeMount } from 'vue';\n\nexport default {\n    props:['playlist'],\n    // setup(props){\n    //     onMounted(()=>console.log(\"listviewtop的传值成功了没？\",props.playlist))\n    // }\n    data:()=>{\n        return{\n            placeholder:\"Racheal Yamagata\",\n            keywordList:['杨千嬅'],\n            searchKeyword:'',\n        }\n    },\n    beforeMount(){\n        // localStorage.keywordList=\"\";\n        console.log(\"localStorage\",localStorage.keywordList)\n        this.keywordList=localStorage.keywordList? JSON.parse(localStorage.keywordList):this.keywordList;\n    },\n    methods:{\n        saveKeyword:function(){\n            this.keywordList.push(this.searchKeyword);\n            localStorage.keywordList=JSON.stringify(this.keywordList);\n        }\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}