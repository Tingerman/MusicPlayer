{"ast":null,"code":"import { reactive, onMounted } from 'vue';\nimport Swiper from 'swiper'; // 引入库\n\nimport 'swiper/css/swiper.css'; // 引入样式文件，注意5和6版本的样式文件不一致\n\nimport icon_right_0 from '@/assets/homedetail/banner.jpg'; //引入的图片\n\nimport icon_right_1 from '@/assets/homedetail/banner.jpg1'; //引入的图片\n\nexport default {\n  name: 'HomeSwiper',\n\n  setup() {\n    onMounted(() => {\n      var mySwiper = new Swiper('.swiper-container', {\n        loop: true,\n        // 循环模式选项\n        autoplay: true,\n        //自动滚动\n        effect: 'fade',\n        //渐变切换\n        autoplay: {\n          delay: 2000,\n          //1秒切换一次\n          pauseOnMouseEnter: true //鼠标移入 轮播暂停\n\n        }\n      });\n    });\n    let imgs = reactive([{\n      pic: icon_right_0\n    }, {\n      pic: icon_right_1\n    }, {\n      pic: icon_right_0\n    }]);\n    return {\n      imgs\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAQC,SAASA,QAAT,EAAmBC,SAAnB,QAAoC,KAApC;AACA,OAAOC,MAAP,MAAmB,QAAnB,C,CAA4B;;AAC5B,OAAO,uBAAP,C,CAA+B;;AAC/B,OAAOC,YAAP,MAAyB,gCAAzB,C,CAA0D;;AAC1D,OAAOC,YAAP,MAAyB,iCAAzB,C,CAA0D;;AAG1D,eAAe;EACdC,IAAI,EAAE,YADQ;;EAEdC,KAAK,GAAG;IACPL,SAAS,CAAC,MAAM;MACf,IAAIM,QAAO,GAAI,IAAIL,MAAJ,CAAW,mBAAX,EAAgC;QAC9CM,IAAI,EAAE,IADwC;QAClC;QACZC,QAAQ,EAAE,IAFoC;QAE9B;QAChBC,MAAM,EAAE,MAHsC;QAG/B;QACfD,QAAQ,EAAE;UACTE,KAAK,EAAE,IADE;UACI;UACbC,iBAAiB,EAAE,IAFV,CAEe;;QAFf;MAJoC,CAAhC,CAAf;IAWA,CAZQ,CAAT;IAaA,IAAIC,IAAG,GAAIb,QAAQ,CAAC,CACnB;MAAEc,GAAG,EAAEX;IAAP,CADmB,EAEnB;MAAEW,GAAG,EAAEV;IAAP,CAFmB,EAGnB;MAAEU,GAAG,EAAEX;IAAP,CAHmB,CAAD,CAAnB;IAKA,OAAO;MACNU;IADM,CAAP;EAGD;;AAxBc,CAAf","names":["reactive","onMounted","Swiper","icon_right_0","icon_right_1","name","setup","mySwiper","loop","autoplay","effect","delay","pauseOnMouseEnter","imgs","pic"],"sourceRoot":"","sources":["/Users/wangmanting/Desktop/云e办/音乐播放器vue项目/musicapp/src/components/swiperCom.vue"],"sourcesContent":["<template>\n\t<div class=\"swiper-container\">\n\t\t<div class=\"swiper-wrapper\">\n\t\t\t<div class=\"swiper-slide\" v-for=\"(item, i) in imgs\" :key=\"i\"><img :src=\"item.pic\" /></div>\n\t\t</div>\n\t</div>\n</template>\n<script>\n\timport { reactive, onMounted } from 'vue'\n\timport Swiper from 'swiper' // 引入库\n\timport 'swiper/css/swiper.css' // 引入样式文件，注意5和6版本的样式文件不一致\n\timport icon_right_0 from '@/assets/homedetail/banner.jpg' //引入的图片\n\timport icon_right_1 from '@/assets/homedetail/banner.jpg1'//引入的图片\n\n\n\texport default {\n\t\tname: 'HomeSwiper',\n\t\tsetup() {\n\t\t\tonMounted(() => {\n\t\t\t\tvar mySwiper = new Swiper('.swiper-container', {\n\t\t\t\t\tloop: true, // 循环模式选项\n\t\t\t\t\tautoplay: true, //自动滚动\n\t\t\t\t\teffect: 'fade',//渐变切换\n\t\t\t\t\tautoplay: {\n\t\t\t\t\t\tdelay: 2000, //1秒切换一次\n\t\t\t\t\t\tpauseOnMouseEnter: true //鼠标移入 轮播暂停\n\n\t\t\t\t\t},\n\n\t\t\t\t})\n\t\t\t})\n\t\t\tlet imgs = reactive([\n\t\t\t\t{ pic: icon_right_0 }, \n\t\t\t\t{ pic: icon_right_1 }, \n\t\t\t\t{ pic: icon_right_0 }\n\t\t\t])\n\t\t\treturn {\n\t\t\t\timgs\n\t\t\t}\n\t\t}\n\t}\n</script>\n<style lang=\"less\">\n\t.swiper-container {\n\t\twidth: 100%;\n\t\theight: 200px;\n\t\t\n\t\t.swiper-slide {\n\t\t\twidth: 100% !important;\n\n\t\t\timg {\n\t\t\t\twidth: 100%;\n\t\t\t\theight: 100%;\n\t\t\t\tobject-fit: cover;\n\t\t\t}\n\t\t}\n\t}\n</style>\n"]},"metadata":{},"sourceType":"module"}